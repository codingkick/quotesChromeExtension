{
  "manifest_version": 3,
  "name": "Quotes",
  "version": "1.0",
  "description": "Show quotes on new tab",
  "permissions": ["storage"],
  "action": {
    "default_icon": "icons/coin.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/coin.png"
  }
}
